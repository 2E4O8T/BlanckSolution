@model Movies.Client.Models.UserInfoViewModel

@{
    ViewData["Title"] = "OnlyAdmin";
}

<h1>OnlyAdmin</h1>

<table class="table">
    <thread>
        <tr>
            <th>
                Claim Type
            </th>
            <th>
                Claim Value
            </th>
        </tr>
    </thread>
    <tboby>
        @foreach (var item in Model.UserInfoDictionary)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
            </tr>
        }
    </tboby>
</table>